{
  "name": "@nexirift/better-auth-plugins",
  "version": "0.0.1",
  "description": "This is a collection of plugins for Better Auth that were built by Nexirift with the intention of being used with Cosmos.",
  "author": "Nexirift",
  "license": "AGPL-3.0-only",
  "type": "module",
  "module": "src/index.ts",
  "scripts": {
    "switch-registry": "chmod +x ./utility-scripts/switch-registry.sh && ./utility-scripts/switch-registry.sh",
    "build": "unbuild",
    "prepack": "unbuild"
  },
  "devDependencies": {
    "eslint": "^9.34.0",
    "eslint-plugin-autofix": "^2.2.0",
    "eslint-plugin-import": "^2.32.0",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.41.0",
    "unbuild": "^3.6.1"
  },
  "peerDependencies": {
    "better-auth": ">=1.3.7",
    "zod": ">=3.24.2"
  },
  "publishConfig": {
    "directory": "dist",
    "access": "public"
  },
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.cjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist/**"
  ],
  "dependencies": {
    "@better-auth/expo": "1.3.8-beta.6"
  }
}
